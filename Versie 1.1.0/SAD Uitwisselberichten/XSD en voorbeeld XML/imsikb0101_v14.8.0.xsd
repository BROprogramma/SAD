                    <element name="extend" minOccurs="0">
                        <annotation>
                            <documentation>is a map polygon delineating the boundaries on the earth's surface.</documentation>
                        </annotation>
                        <complexType>
                            <complexContent>
                                <extension base="gml:AbstractMemberType">
                                    <sequence minOccurs="0">
                                        <element ref="gml:PolygonPatch"/>
